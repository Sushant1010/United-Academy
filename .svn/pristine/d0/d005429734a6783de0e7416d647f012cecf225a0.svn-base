@model United_IMS.Web.Models.ACD_Class

@{
    ViewBag.Title = "Details";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<section class="content-header">
    <h1>Academic Class Details</h1>
    <ol class="breadcrumb"><li class="list">@Html.ActionLink("List", "Index",null,new{@class= "btn btn-xs btn-success" })</li></ol>
</section>

<section class="content">
<div class="box box-success">
<div class="box-body">
@using (Html.BeginForm()) 
{
    @Html.AntiForgeryToken()
    
    
        @Html.ValidationSummary(true, "", new { @class = "text-danger" })
        <div class="form-group row">
		<div class="col-sm-2">
            @Html.LabelFor(model => model.ClassName, new { @class = "control-label" })
            </div>
			<div class="col-sm-3">
                @Html.TextBoxFor(model => model.ClassName,  new { @class = "form-control",disabled="disabled" } )
                @Html.ValidationMessageFor(model => model.ClassName, "", new { @class = "text-danger" })
            </div>
        </div>

       

        <div class="form-group row">
		<div class="col-sm-2">
            @Html.LabelFor(model => model.ClassCode, new { @class = "control-label" })
            </div>
			<div class="col-sm-3">
                @Html.TextBoxFor(model => model.ClassCode,  new { @class = "form-control",disabled="disabled" } )
                @Html.ValidationMessageFor(model => model.ClassCode, "", new { @class = "text-danger" })
            </div>
        </div>
		<div class="form-group row">
			<div class="col-sm-2">
				@Html.LabelFor(model => model.OrganizationId, new { @class = "control-label" })
			</div>
			<div class="col-sm-3">
				@Html.DropDownList("OrganizationId",null,"-- Select --",new { @class="form-control",disabled="disabled"})
			</div>
		</div>

       

}
</div>
</div>
</section>
